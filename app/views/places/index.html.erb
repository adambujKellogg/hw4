<% if @current_user %>
<h1>My Places</h1>
  
<ul>
  <% for place in @places %>
    <li><a href="/places/<%= place["id"] %>"><%= place["name"] %></a></li>
  <% end %>
</ul>
<p>
    <a class="btn btn-primary btn-sm mt-2" href="/places/new">Add Place</a>
</p>
<% else %>
  Please log in to view places
<% end %>






